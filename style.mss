#tmworldborders0 {
  text-face-name: "Comic Sans MS Regular";
  text-fill: #000;
  text-size:14;
  text-transform:uppercase;
  text-halo-fill:rgba(255,255,255,0.5);
  text-halo-radius:3;
  text-line-spacing:1;
  text-wrap-width:20;
  text-name: "'Wow'";
  text-placement: interior;
  line-color: rgb(121, 96, 83);

  [NAME="United States"] {
    text-name: "'Much State'";
  }
}

#dogepoints {
  point-allow-overlap: true;
  [name="Dogecoin"] {
    point-file: url('Dogecoin_logo.png');
    point-ignore-placement: true;
  }
  [name="Dogehouse"] {
    point-file: url('dogHouse.png');
  }
  [name="ShibaInu"] {
    point-file: url('shibainu.png');
  }
  [name="Pizza"] {
    point-file: url('Pizza_slice_icon.png');
  }
  [name="Bobsled"] {
    point-file: url('bobsled.png');
  }
}

